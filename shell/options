# ZSH
DISABLE_AUTO_UPDATE="true"

# SHELL
PROFILE="$HOME/.zshrc"

# LOCALE
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# GLOBAL
export EDITOR="nvim"
export GIT_EDITOR="/Applications/Emacs.app/Contents/MacOS/bin/emacsclient -c --no-wait"

# TMUX
# https://github.com/robbyrussell/oh-my-zsh/wiki/Plugins#tmux
export ZSH_TMUX_AUTOSTART="true"
export ZSH_TMUX_AUTOSTART_ONCE="true"

# oh-my-zsh
export DISABLE_UPDATE_PROMPT=true

# POSTGRESQL
export PGHOST=localhost
export PGUSER=postgres

# Shell history in IEx
export ERL_AFLAGS="-kernel shell_history enabled"

# GESTPREV USER
export EMAIL1=jsvitolo@me.com
export PASS1=!sisprev@789@
export NAME1="Jones Silva Vitolo"
